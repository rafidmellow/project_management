{
  "name": "project_management",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/verify-prisma.js && node scripts/build.js",
    "build:safe": "node scripts/build.js",
    "build:direct": "node scripts/direct-build.js",
    "build:minimal": "npx cross-env NEXT_WEBPACK_DISABLE_WATCHING=1 CHOKIDAR_USEPOLLING=false npx prisma generate && npx next build",
    "build:server": "node scripts/server-build.js",
    "start": "next start",
    "start:pm2": "pm2 start npm --name \"project-management\" -- start",
    "start:custom": "node server.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "db:setup": "node scripts/setup-db.js",
    "db:seed": "node scripts/seed.js",
    "db:seed-permissions": "node scripts/seed-permissions.js",
    "db:update-permissions": "node scripts/update-edge-permissions-fixed.js",
    "db:reset": "npm run db:setup && npx prisma migrate reset --force && npm run db:seed && npm run db:seed-permissions && npm run db:update-permissions",
    "db:validate": "node scripts/validate-db.js",
    "prisma:generate": "npx prisma generate",
    "prisma:regenerate": "node scripts/regenerate-prisma.js",
    "postinstall": "npx prisma generate",
    "prepare": "husky",
    "setup:production": "node scripts/setup-production.js"
  },
